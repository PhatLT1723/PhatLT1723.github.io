<div class="container">
    <div class="row">
        <form action="<%=create_group_path%>">
            <div class="col s12">
                <span style="color: gray">GroupName:</span>
                <input type="text" name="groupName" value="<%= group.groupName %>">
            </div>
            <div class="col s12">
                <span style="color: gray">Password:</span>
                <input type="password" name="password" value="<%= group.password %>">
            </div>
            <div class="col s12">
                <span style="color: gray">SubId:</span>
                <input type="text" name="subId" value="<%= group.subId %>">
            </div>
            <div class="col s12">
                <span style="color: gray">UserId:</span>
                <input type="text" name="userId" value="<%= group.userId %>">
            </div>
            <div class="col s12 center">
                <input type="submit" name="Submit" class="btn btn-info">
            </div>
        </form>
    </div>
</div>
