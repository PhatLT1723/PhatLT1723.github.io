<div class="container">
    <div class="row">
        <form action="<%=update_group_path%>">
            <div class="col s12">
                <span style="color: gray">Group Name:</span>
                <input type="text" name="groupName" value="<%= group.groupName %>">
            </div>
            <div class="col s12">
              <span style="color: gray">Sub Id:</span>
                <input type="text" name="subId" value="<%= group.subId %>">
            </div>
            <div class="col s12">
              <span style="color: gray">Password:</span>
                <input type="password" name="password" value="<%= group.password %>">
            </div>
            <div class="col s12">
                <input type="hidden" name="id" value="<%= group.groupId %>">
            </div>
            <div class="col s12 center">
                <input type="submit" name="Submit" class="btn btn-info">
            </div>
        </form>
    </div>
</div>
